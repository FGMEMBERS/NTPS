<layers>
 
<!--   AI Radar echos    -->

<layer>
  <name>AI-0</name>
  <condition>
   <property>/instrumentation/radar/ai[0]/active</property>
   <less-than>
   <property>/instrumentation/radar/ai[0]/norm-dist</property>
   <value>1.000</value>
   </less-than>
  </condition>
  <type>text</type>
  <font>default</font>
  <point-size>8</point-size>
  <color>
    <red>0.8</red>
    <green>0.8</green>
    <blue>0.8</blue>
  </color>
  <chunks>
   <chunk>
  <type>literal</type>
  <text>. </text>
    </chunk>
   <chunk>
  <type>text-value</type>
  <property>/instrumentation/radar/ai[0]/id</property>
    </chunk>
  </chunks>
  <transformations>
     <transformation>       
      <type>rotation</type>
      <property>/instrumentation/radar/ai[0]/brg-offset</property>
      <min>-180.0</min>
      <max>180.0</max>
      <scale>1.0</scale>
	  </transformation>
	 <transformation>
      <type>y-shift</type>
      <property>/instrumentation/radar/ai[0]/norm-dist</property>
    	  <scale>160.0</scale>
		  </transformation>
     <transformation>       
      <type>rotation</type>
